# Production Environment Variables
# Copy this to .env.production.local and fill in your actual values

# Backend GraphQL API URL (update with your Cloud Run service URL)
VITE_GRAPHQL_URL=https://your-backend-service-url.run.app/graphql/

# WorkOS Configuration
VITE_WORKOS_CLIENT_ID=your_production_workos_client_id

# Production redirect URI (update with your App Engine URL)
VITE_WORKOS_REDIRECT_URI=https://console-dot-your-project-id.uc.r.appspot.com/auth/callback
